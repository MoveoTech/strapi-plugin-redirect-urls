{
  "name": "strapi-plugin-redirect-urls",
  "version": "1.0.0",
  "description": "Manage URL redirects from within the Strapi admin panel",
  "keywords": [
    "strapi",
    "plugin",
    "redirects",
    "url-redirect",
    "seo",
    "url-management",
    "strapi-plugin"
  ],
  "type": "commonjs",
  "main": "./dist/server/index.js",
  "module": "./dist/server/index.mjs",
  "types": "./dist/server/src/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    "./strapi-admin": {
      "types": "./dist/admin/src/index.d.ts",
      "source": "./admin/src/index.ts",
      "import": "./dist/admin/index.mjs",
      "require": "./dist/admin/index.js",
      "default": "./dist/admin/index.js"
    },
    "./strapi-server": {
      "types": "./dist/server/src/index.d.ts",
      "source": "./server/src/index.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "strapi-plugin build",
    "watch": "strapi-plugin watch",
    "watch:link": "strapi-plugin watch:link",
    "verify": "strapi-plugin verify",
    "test:ts:front": "tsc -p admin/tsconfig.json",
    "test:ts:back": "tsc -p server/tsconfig.json",
    "prepublishOnly": "npm run build",
    "format": "prettier --continue -- --cache --cache-location='node_modules/.cache/.prettiercache'",
    "format:fix": "prettier --write",
    "test": "yarn test:ts:front && yarn test:ts:back"
  },
  "dependencies": {
    "@strapi/design-system": "^2.0.0-rc.30",
    "@strapi/icons": "^2.0.0-rc.30",
    "react-intl": "^7.1.14"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@strapi/eslint-config": "^0.2.1",
    "@strapi/sdk-plugin": "^5.3.2",
    "@strapi/strapi": "^5.31.2",
    "@strapi/typescript-utils": "^5.31.2",
    "@types/minimatch": "^5.1.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.6.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1",
    "styled-components": "^6.1.19",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "@strapi/sdk-plugin": "^5.3.2",
    "@strapi/strapi": "^5.31.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1",
    "styled-components": "^6.1.19"
  },
  "strapi": {
    "kind": "plugin",
    "name": "custom-redirects-plugin",
    "displayName": "Redirects",
    "description": "Manage URL redirects from within the Strapi admin panel"
  },
  "resolutions": {
    "glob": "^10.5.0"
  },
  "license": "MIT",
  "author": {
    "name": "Daniel Yaakov",
    "email": "daniely@moveo.co.il"
  },
  "maintainers": [
    {
      "name": "Daniel Yaakov",
      "email": "daniely@moveo.co.il"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/MoveoTech/strapi-plugin-redirect-urls.git"
  },
  "bugs": {
    "url": "https://github.com/MoveoTech/strapi-plugin-redirect-urls/issues"
  },
  "homepage": "https://github.com/MoveoTech/strapi-plugin-redirect-urls#readme",
  "engines": {
    "node": ">=18.0.0 <=20.x.x",
    "npm": ">=6.0.0"
  }
}
